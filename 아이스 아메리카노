class Solution {
    fun solution(money: Int): IntArray {
        var answer = mutableListOf<Int>()
        answer.add(money/5500)
        answer.add(money%5500)
        return answer.toIntArray()
    }
}

class Solution {
    fun solution(money: Int): IntArray {
        var answer = intArrayOf(money/5500,money%5500)
        return answer
    }
}
