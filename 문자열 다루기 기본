class Solution {
    fun solution(s: String): Boolean {
        var answer = true
        if(s.length==4 || s.length==6){
                try{
                    if(s.toInt()>=0)
                    answer = true
                }
                catch(e:java.lang.NumberFormatException){
                answer = false
                }
        }
        else{
            answer = false
        }
        return answer
}
}
